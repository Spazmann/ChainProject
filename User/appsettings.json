{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MessageAPIServiceDBMongo": {
    "ConnectionString": "mongodb://MessageAPIServiceDBMongo:27017",
    "DatabaseName": "MessageDB",
    "BooksCollectionName": "users"
